{
  "name": "sarehub/service-builder",
  "type": "composer-plugin",
  "description": "service builder composer plugin",
  "minimum-stability": "stable",
  "license": "proprietary",
  "repositories": [
    {
      "type": "vcs",
      "url":  "git@github.com:SAREhub/service-builder.git"
    }
  ],
  "authors": [
    {
      "name": "Jakub Ca≈Ça",
      "email": "j.cala@sare.pl"
    }
  ],
  "require-dev": {
    "phpunit/phpunit": "6.*",
    "mockery/mockery": "1.*",
    "composer/composer": "dev-master"
  },
  "require": {
    "php": "^7.0",
    "composer-plugin-api": "^1.0",
    "vendor/service-builder": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "SAREhub\\": "src/SAREhub"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SAREhub\\": "tests/SAREhub"
    }
  },
  "extra": {
    "class": "SAREhub\\Plugin\\ServiceBuilder\\ServiceBuilderPlugin"
  }
}